# Environment Variables for N8N Parity

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/flexoraa
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key

# JWT
JWT_SECRET=your-jwt-secret-minimum-32-characters-required
JWT_REFRESH_SECRET=your-refresh-secret-minimum-32-characters

# Encryption
ENCRYPTION_KEY=your-64-character-hex-encryption-key

# OpenAI (ChatGPT) - REQUIRED FOR PARITY
OPENAI_API_KEY=sk-your-openai-api-key

# Pinecone (RAG)
PINECONE_API_KEY=your-pinecone-key
PINECONE_ENVIRONMENT=your-environment
PINECONE_INDEX=your-index-name

# WhatsApp Cloud API - REQUIRED FOR PARITY
META_PHONE_NUMBER_ID=your-phone-number-id
META_ACCESS_TOKEN=your-whatsapp-access-token
META_APP_SECRET=your-app-secret
META_VERIFY_TOKEN=your-custom-verify-token

# Twilio (Alternative SMS)
TWILIO_ACCOUNT_SID=your-twilio-sid
TWILIO_AUTH_TOKEN=your-twilio-token
TWILIO_PHONE_NUMBER=+15555551234

# KlickTipp - REQUIRED FOR PARITY
KLICKTIPP_USERNAME=your-klicktipp-username
KLICKTIPP_PASSWORD=your-klicktipp-password
KLICKTIPP_OPTOUT_LIST_ID=your-opt-out-list-id

# Redis
REDIS_URL=redis://localhost:6379

# Server
PORT=3000
NODE_ENV=production
LOG_LEVEL=info
