{
  "name": "flexoraa-backend-api",
  "version": "1.0.0",
  "description": "Production-ready Express.js backend replacing n8n workflows",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:local": "cross-env NODE_ENV=test LOCAL_MODE=true jest --runInBand --config jest.local.config.js",
    "test:watch": "jest --watch",
    "test:contracts": "jest --testPathPattern=tests/contracts",
    "test:e2e": "playwright test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "node scripts/run-migrations.js",
    "generate-openapi": "node scripts/generate-openapi.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@pinecone-database/pinecone": "^1.1.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.20.1",
    "opossum": "^8.1.3",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "rate-limit-redis": "^4.3.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
